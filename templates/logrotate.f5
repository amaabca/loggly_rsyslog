# {{ ansible_managed }}
/var/log/f5/*.log {
  su root root
  daily
  rotate 3
  missingok
  notifempty
  delaycompress
  sharedscripts
  firstaction
    service rsyslog stop
  endscript
  postrotate
    # Delete Rsyslog state for the rotated files
    rm /var/spool/rsyslog/imfile-state*
  endscript
  lastaction
    service rsyslog start
  endscript
}
